{
  "name": "Deep Tab",
  "version": "2.0.0",
  "description": "一款漂亮的tab插件",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "unlimitedStorage",
    "tabs",
    "activeTab",
    "alarms",
    "notifications",
    "topSites",
    "bookmarks",
    "scripting"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "extension/background/index.js"
  },
  "action": {
    "default_title": "Deep Tab标签",
    "default_icon": "src/assets/images/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "src/assets/images/icon16.png",
    "48": "src/assets/images/icon48.png",
    "128": "src/assets/images/icon128.png"
  }
}
